		<xsd:complexContent>
			<xsd:extension base="tns:couchTransactionType">
				<xsd:all minOccurs="0">
					<xsd:element name="couchdb">
						<xsd:complexType>
							<xsd:all minOccurs="0">
								<xsd:element name="auth_cache_misses" type="tns:couchServerStatType"/>
								<xsd:element name="database_writes" type="tns:couchServerStatType"/>
								<xsd:element name="open_databases" type="tns:couchServerStatType"/>
