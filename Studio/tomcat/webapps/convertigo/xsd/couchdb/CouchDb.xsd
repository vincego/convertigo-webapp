								<xsd:element name="COPY" type="tns:couchServerStatType"/>
								<xsd:element name="DELETE" type="tns:couchServerStatType"/>
								<xsd:element name="POST" type="tns:couchServerStatType"/>
								<xsd:element name="HEAD" type="tns:couchServerStatType"/>
							</xsd:all>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="httpd_status_codes">
						<xsd:complexType>
							<xsd:all minOccurs="0">
								<xsd:element name="_403" type="tns:couchServerStatType"/>
								<xsd:element name="_202" type="tns:couchServerStatType"/>
								<xsd:element name="_401" type="tns:couchServerStatType"/>
								<xsd:element name="_409" type="tns:couchServerStatType"/>
								<xsd:element name="_200" type="tns:couchServerStatType"/>
								<xsd:element name="_405" type="tns:couchServerStatType"/>
								<xsd:element name="_400" type="tns:couchServerStatType"/>
								<xsd:element name="_201" type="tns:couchServerStatType"/>
								<xsd:element name="_404" type="tns:couchServerStatType"/>
								<xsd:element name="_500" type="tns:couchServerStatType"/>
								<xsd:element name="_412" type="tns:couchServerStatType"/>
								<xsd:element name="_301" type="tns:couchServerStatType"/>
								<xsd:element name="_304" type="tns:couchServerStatType"/>
							</xsd:all>
