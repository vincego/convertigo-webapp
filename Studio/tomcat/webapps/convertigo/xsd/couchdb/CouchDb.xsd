						<xsd:complexType>
							<xsd:all>
								<xsd:element name="version" type="xsd:string"/>
								<xsd:element name="name" type="xsd:string"/>
							</xsd:all>
						</xsd:complexType>
					</xsd:element>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="getServerLogType">
		<xsd:complexContent>
			<xsd:extension base="tns:couchTransactionType">
				<xsd:all minOccurs="0">
					<xsd:element name="data" type="xsd:string"/>
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
